{% extends 'base.html.twig' %}

{% block title %}Enseignements index{% endblock %}

{% block body %}
    <h1>Enseignements index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Designation</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>Slug</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for enseignement in enseignements %}
            <tr>
                <td>{{ enseignement.id }}</td>
                <td>{{ enseignement.designation }}</td>
                <td>{{ enseignement.createdAt ? enseignement.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ enseignement.updatedAt ? enseignement.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ enseignement.slug }}</td>
                <td>
                    <a href="{{ path('app_enseignements_show', {'id': enseignement.id}) }}">show</a>
                    <a href="{{ path('app_enseignements_edit', {'id': enseignement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_enseignements_new') }}">Create new</a>
{% endblock %}
