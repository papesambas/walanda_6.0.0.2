{% extends 'base.html.twig' %}

{% block title %}Telephones{% endblock %}

{% block body %}
    <h1>Telephones</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ telephone.id }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ telephone.numero }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ telephone.createdAt ? telephone.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ telephone.updatedAt ? telephone.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ telephone.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_telephones_index') }}">back to list</a>

    <a href="{{ path('app_telephones_edit', {'id': telephone.id}) }}">edit</a>

    {{ include('telephones/_delete_form.html.twig') }}
{% endblock %}
