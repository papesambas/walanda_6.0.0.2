{% extends 'base.html.twig' %}

{% block title %}Prenoms{% endblock %}

{% block body %}
    <h1>Prenoms</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prenom.id }}</td>
            </tr>
            <tr>
                <th>Designation</th>
                <td>{{ prenom.designation }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ prenom.createdAt ? prenom.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ prenom.updatedAt ? prenom.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ prenom.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_prenoms_index') }}">back to list</a>

    <a href="{{ path('app_prenoms_edit', {'id': prenom.id}) }}">edit</a>

    {{ include('prenoms/_delete_form.html.twig') }}
{% endblock %}
