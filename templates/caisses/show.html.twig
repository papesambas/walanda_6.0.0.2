{% extends 'base.html.twig' %}

{% block title %}Caisses{% endblock %}

{% block body %}
    <h1>Caisses</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ caiss.id }}</td>
            </tr>
            <tr>
                <th>DateOp</th>
                <td>{{ caiss.dateOp ? caiss.dateOp|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ caiss.libelle }}</td>
            </tr>
            <tr>
                <th>Debit</th>
                <td>{{ caiss.debit }}</td>
            </tr>
            <tr>
                <th>Credit</th>
                <td>{{ caiss.credit }}</td>
            </tr>
            <tr>
                <th>Solde</th>
                <td>{{ caiss.solde }}</td>
            </tr>
            <tr>
                <th>SoldeCumul</th>
                <td>{{ caiss.soldeCumul }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ caiss.createdAt ? caiss.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ caiss.updatedAt ? caiss.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ caiss.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_caisses_index') }}">back to list</a>

    <a href="{{ path('app_caisses_edit', {'id': caiss.id}) }}">edit</a>

    {{ include('caisses/_delete_form.html.twig') }}
